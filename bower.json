{
  "name": "podlove-web-player",
  "homepage": "http://podlove.org/",
  "description": "Next generation media player for the web. Special features for podcast and other metadata rich productions.",
  "main": [
    "src/index.html",
    "src/js/app.js",
    "src/js/moderator.js",
    "src/sass/pwp-dark.scss"
  ],
  "ignore": [
    "**/.*",
    "karma.*",
    "config.rb",
    "package.json",
    "node_modules",
    "bower_components",
    "spec"
  ],
  "moduleType": [
