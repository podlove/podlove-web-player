    "podcasting",
    "podplayer",
    "player",
    "webplayer",
    "media",
    "audio",
    "web"
  ],
  "license": "BSD-2-Clause",
  "dependencies": {},
  "devDependencies": {
    "html5shiv": "~3.7.2",
    "progress-polyfill": "https://github.com/LeaVerou/HTML5-Progress-polyfill.git",
    "jquery": "~1.11.1",
    "mediaelement": "~2.16.0",
    "podlove-font": "#2.0.0-beta.2",
    "raf-polyfill": "https://gist.github.com/1579671.git",
    "jquery.i18n": "https://github.com/wikimedia/jquery.i18n.git"
  }
